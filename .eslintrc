{
  "plugins": ["prettier", "jest"],
  "extends": ["airbnb-base", "prettier", "plugin:jest/recommended"],
  "env": {
    "jest/globals": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "strict": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id", "_rev"] }],
    "global-require": "off",
    "import/no-unresolved": "off"
  }
}
